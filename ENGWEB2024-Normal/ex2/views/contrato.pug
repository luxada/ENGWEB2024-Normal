extends layout

block content
  h2.w3-center Detalhes do Contrato
  div.w3-card-4
    div.w3-container.w3-teal
      h3= contrato.objectoContrato
    div.w3-container
      ul.w3-ul
        li
          span.w3-teal ID do Contrato:
          |  #{contrato._id}
        li
          span.w3-teal Nº de Anuncio: 
          |  #{contrato.nAnuncio}
        li
          span.w3-teal Tipo de Procedimento: 
          |  #{contrato.tipoprocedimento}
        li
          span.w3-teal Data de Publicação: 
          |  #{contrato.dataPublicacao}
        li
          span.w3-teal Data de Celebração:
          |  #{contrato.dataCelebracaoContrato}
        li
          span.w3-teal Preço Contratual:
          |  #{contrato.precoContratual}
        li 
          span.w3-teal Prazo de Execução:
          |  #{contrato.prazoExecucao}
        li
          span.w3-teal NIPC da Entidade Comunicante:
          |  #{contrato.NIPC_entidade_comunicante}
        li
          span.w3-teal Entidade Comunicante:
          |  #{contrato.entidade_comunicante}
        li
          span.w3-teal Fundamentação:
          |  #{contrato.fundamentacao}
    div.w3-container.w3-teal
      a.w3-btn.w3-ripple.w3-teal(href="/") Voltar à página principal